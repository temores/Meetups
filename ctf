SecurityEvent 
| where TimeGenerated >= todatetime('2019-07-16T20:33:28Z') 
| where EventID == 4688 
| where Account contains @"RonHD" 
| where Computer contains @"AdminPC"
| project TimeGenerated, CommandLine , NewProcessName
| order by TimeGenerated asc

