SecurityEvent
| where TimeGenerated >= todatetime('2019-07-16T00:00:00Z')
| where EventID == 4624
| where Computer contains @"VictimPC" or Computer contains @"AdminPC"
| where AccountType == 'User'
| order by TimeGenerated asc


